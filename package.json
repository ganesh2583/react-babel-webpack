{
  "name": "react-babel-webpack",
  "version": "1.0.0",
  "description": "Sample React Babel 7 and Webpack 4 project with SASS.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.js --mode development --hot --history-api-fallback",
    "test": "jest",
    "build:prod": "webpack -p --progress --config=webpack.prod.js --mode production",
    "start:prod": "webpack -p --progress --config=webpack.prod.js --mode production && babel-node server.js"
  },
  "author": "Ganesh Chaitanya Kale",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.1",
    "express": "^4.17.1",
    "file-loader": "^5.0.2",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "dotenv-webpack": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "keywords": [
    "react",
    "webpack",
    "babel"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
